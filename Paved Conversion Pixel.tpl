const createQueue = require('createQueue');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const logToConsole = require('logToConsole');
const makeTableMap = require('makeTableMap');
const getTimestamp = require('getTimestampMillis');
const Math = require('Math');

const pixelId = data.pixelId;
const now = getTimestamp();
const dayTs = 86400000 * Math.ceil(now / 86400000);
const scriptUrl = 'https://pvdpix.com/pixel.js?t=' + dayTs;

const eventName = (data.eventType === 'custom' && data.customEventName)
  ? data.customEventName
  : data.eventType;

const buildEventParams = () => {
  const params = {};

  switch (data.eventType) {
    case 'purchase':
      if (data.currency) params.currency = data.currency;
      if (data.value) params.value = data.value * 1;
      break;
    case 'sign_up':
      if (data.accountType) params.account_type = data.accountType;
      break;
    case 'download':
      if (data.file) params.file = data.file;
      break;
    case 'search':
      if (data.searchTerm) params.search_term = data.searchTerm;
      break;
  }

  if (data.customParameters && data.customParameters.length > 0) {
    const customParams = makeTableMap(data.customParameters, 'name', 'value');
    for (let key in customParams) params[key] = customParams[key];
  }

  if (data.enableEnhancedUserMatching) {
    if (data.email) params.email = data.email.toLowerCase();
    if (data.md5Email) params.md5_email = data.md5Email;
    if (data.sha256Email) params.sha256_email = data.sha256Email;
  }

  return params;
};

let pvd = copyFromWindow('pvd');

if (!pvd) {
  setInWindow('pvd', function() {
    (copyFromWindow('pvd.queue') || []).push(arguments);
  });
  createQueue('pvd.queue');
  pvd = copyFromWindow('pvd');
}

if (!copyFromWindow('pvd.initialized')) {
  pvd('init', pixelId);
  setInWindow('pvd.initialized', true);
  logToConsole('[paved] Pixel initialized:', pixelId);
}

const sendEvent = () => {
  const realPvd = copyFromWindow('pvd');
  if (realPvd && typeof realPvd.process === 'function') {
    realPvd.process('init', pixelId);
    realPvd.process('event', eventName, buildEventParams());
    logToConsole('[paved] ✅ Event sent directly via real process method with pixelId:', pixelId);
  } else {
    pvd('init', pixelId);
    pvd('event', eventName, buildEventParams());
    logToConsole('[paved] ✅ Event queued (script not loaded yet)');
  }
};

if (!copyFromWindow('pvd.loaded')) {
  injectScript(
    scriptUrl,
    () => {
      setInWindow('pvd.loaded', true);
      logToConsole('[paved] ✅ Pixel script loaded');
      sendEvent();
      data.gtmOnSuccess();
    },
    () => {
      logToConsole('[paved] ❌ Failed to load pixel script');
      data.gtmOnFailure();
    },
    'pvdPixel'
  );
} else {
  sendEvent();
  data.gtmOnSuccess();
}


